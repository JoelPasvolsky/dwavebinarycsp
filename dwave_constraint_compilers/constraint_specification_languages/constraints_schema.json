{
  "$schema": "http://json-schema.org/schema#",

  "type": "object",

  "properties": {
    "all_variables": {
      "type": "array",
      "items": {
        "type": [
          "number",
          "string"
        ]
      }
    }
  },

  "patternProperties": {
    "^(?!all_variables).*$": {
      "type": "object",
      "properties": {
        "feasible_configurations": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        },
        "variables": {
          "type": "array",
          "items": {
            "type": [
              "number",
              "string"
            ]
          }
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}

